## declare variables
number = 600851475143 
x = 2
#define prime number array
prime = [] 

## find all the prime number  
while True  :
  if number % x == 0 and number / x > 1 :
    print (x)
    number = number / x
    prime.append (x)
  elif number / x == 1 :
    break
  x += 1

print ("These are all the prime numbers\n",prime,int(number))
print ("Largest prime number is :",number)
